{
  "vim.vimrc.enable": false,
  "vim.vimrc.path": "c:\\Users\\PROMETHEUS42\\Documents\\vscode.vim",
  "vim.useSystemClipboard": true,
  "vim.surround": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-c>": false
  },
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": ["#00f22c", "#000"],
  "vim.statusBarColors.insert": ["#001eff", "#fff"],
  "vim.statusBarColors.visual": ["#ff1100", "#fff"],
  "vim.statusBarColors.visualline": ["#B48EAD", "#000"],
  "vim.statusBarColors.visualblock": ["#A3BE8C", "#000"],
  "vim.statusBarColors.replace": "#D08770",
  "vim.statusBarColors.commandlineinprogress": "#007ACC",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "workbench.colorCustomizations": {
    "statusBar.background": "#00f22c",
    "statusBar.noFolderBackground": "#00f22c",
    "statusBar.debuggingBackground": "#00f22c",
    "statusBar.foreground": "#000",
    "statusBar.debuggingForeground": "#000"
  },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.operatorPendingModeKeyBindings": [],
  "vim.operatorPendingModeKeyBindingsNonRecursive": [],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-h>"],
      "after": ["<Left>"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<Down>"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<Up>"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<Right>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.triggerSuggest"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.splitEditor"]
    }
  ],
  "vim.visualModeKeyBindings": [],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true
  },
  "workbench.colorTheme": "Andromeda Colorizer",
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "Anonymice Nerd Font Mono",
  "files.autoSave": "onFocusChange",
  "editor.fontWeight": "normal",
  "editor.fontSize": 20,
  "editor.fontLigatures": true,
  "editor.wordWrap": "on",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "htmlhint.enable": true,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontFamily": "Anonymice Nerd Font Mono",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "editor.lineNumbers": "relative",
  "workbench.editor.untitled.hint": "hidden",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "background.editorBackgrounds": [],
  "background.backgroundOpacity": [0.8, 0.3, 0.9, 0.9],
  "background.backgroundSize": ["Cover", "Cover", "Cover", "Cover"],
  "background.windowBackgrounds": [
    "d:/GameRelated/lol wallpaper/wp/LOL_CMS_181_Article_05_ww64t0gy5o0evz209tzt.jpg"
  ],
  "background.backgroundBlur": ["0.1", "0", "0", "0"],
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "razor.trace": "Verbose",
  "omnisharp.sdkIncludePrereleases": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "vim.camelCaseMotion.enable": true,
  "explorer.compactFolders": false,
  "svelte.enable-ts-plugin": true,
  "git.openRepositoryInParentFolders": "never",
  "[python]": {
    "editor.formatOnType": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "csharpier.enableDebugLogs": true,
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  }
}
